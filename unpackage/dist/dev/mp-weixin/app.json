{
  "pages": [
    "pages/index/index",
    "pages/cate/cate",
    "pages/cart/cart",
    "pages/profile/profile"
  ],
  "subPackages": [
    {
      "root": "subpkg",
      "pages": [
        "Address/Address",
        "order/order",
        "add-edit/add-edit",
        "userinfo/userinfo",
        "create-order/create-order",
        "OrderDetail/OrderDetail",
        "balance/balance",
        "redPacket/redPacket",
        "choose-ticket/choose-ticket",
        "card/card",
        "error-result/error-result",
        "balance-detail/balance-detail"
      ]
    },
    {
      "root": "catepkg",
      "pages": [
        "subCate/subCate"
      ]
    },
    {
      "root": "indexpkg",
      "pages": [
        "login/login",
        "search/search",
        "goods/goods"
      ]
    },
    {
      "root": "goodsPages",
      "pages": [
        "brand/brand",
        "comment/comment"
      ]
    }
  ],
  "usingComponents": {
    "van-action-sheet": "/wxcomponents/vant/action-sheet/index",
    "van-button": "/wxcomponents/vant/button/index",
    "van-cascader": "/wxcomponents/vant/cascader/index",
    "van-cell": "/wxcomponents/vant/cell/index",
    "van-cell-group": "/wxcomponents/vant/cell-group/index",
    "van-checkbox": "/wxcomponents/vant/checkbox/index",
    "van-count-down": "/wxcomponents/vant/count-down/index",
    "van-dialog": "/wxcomponents/vant/dialog/index",
    "van-field": "/wxcomponents/vant/field/index",
    "van-icon": "/wxcomponents/vant/icon/index",
    "van-popup": "/wxcomponents/vant/popup/index",
    "van-radio": "/wxcomponents/vant/radio/index",
    "van-radio-group": "/wxcomponents/vant/radio-group/index",
    "van-search": "/wxcomponents/vant/search/index",
    "van-stepper": "/wxcomponents/vant/stepper/index",
    "van-submit-bar": "/wxcomponents/vant/submit-bar/index",
    "van-swipe-cell": "/wxcomponents/vant/swipe-cell/index",
    "van-switch": "/wxcomponents/vant/switch/index",
    "van-tab": "/wxcomponents/vant/tab/index",
    "van-tabs": "/wxcomponents/vant/tabs/index",
    "van-toast": "/wxcomponents/vant/toast/index",
    "van-datetime-picker": "/wxcomponents/vant/datetime-picker/index"
  },
  "window": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "selectedColor": "#d81e06",
    "list": [
      {
        "text": "首页",
        "pagePath": "pages/index/index",
        "selectedIconPath": "static/home-ac.png",
        "iconPath": "static/home.png"
      },
      {
        "text": "分类",
        "pagePath": "pages/cate/cate",
        "selectedIconPath": "static/cate-ac.png",
        "iconPath": "static/cart.png"
      },
      {
        "text": "购物车",
        "pagePath": "pages/cart/cart",
        "selectedIconPath": "static/cart-ac.png",
        "iconPath": "static/cart.png"
      },
      {
        "text": "个人",
        "pagePath": "pages/profile/profile",
        "selectedIconPath": "static/my-active.png",
        "iconPath": "static/my.png"
      }
    ]
  },
  "preloadRule": {
    "pages/profile/profile": {
      "network": "all",
      "packages": [
        "subpkg"
      ]
    },
    "pages/cate/cate": {
      "network": "all",
      "packages": [
        "catepkg"
      ]
    },
    "pages/index/index": {
      "network": "all",
      "packages": [
        "indexpkg"
      ]
    },
    "indexpkg/goods/goods": {
      "network": "all",
      "packages": [
        "goodsPages"
      ]
    }
  },
  "lazyCodeLoading": "requiredComponents"
}